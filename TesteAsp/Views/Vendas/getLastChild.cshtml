
@model IEnumerable<TesteAsp.Controllers.VendasController.ViewLastChild>

<table class="table">
    <tr>

        <th>
            @Html.DisplayNameFor(model => model.cod_venda)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.empresa)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.cod_produto)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.qtd)
        </th>

        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.cod_venda)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.empresa)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.cod_produto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.qtd)
            </td>
            <td class="divider">
                
            </td>
        </tr>
    }

</table>


